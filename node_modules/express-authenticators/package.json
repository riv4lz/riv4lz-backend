	"repository": "github:tranvansang/express-authenticators",
	"author": "Tran Sang <info@transang.me>",
	"license": "MIT",
	"keywords": [
		"passport",
		"passportjs",
		"express",
		"authenticator",
		"authentication",
		"github",
		"twitter",
		"google",
		"facebook",
		"pinterest",
		"foursquare",
		"linkedin",
		"instagram",
		"tumblr",
		"oauth",
		"oauth2"
	],
	"files": [
		"lib"
	],
	"scripts": {
